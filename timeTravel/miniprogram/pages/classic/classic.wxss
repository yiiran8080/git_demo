/* miniprogram/pages/classic/classic.wxss */
.container{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.header{
  width:100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
}
.magazine{
  margin-top: 4rpx;
  margin-left: 20rpx;
}
.favor{
  margin-top: 6rpx;
}
.nav{
  position: absolute;
  bottom: 40rpx;
}
.like-container{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-right:30rpx;
}

.share-btn {
  margin-top: 10rpx;
  margin-left: 20rpx;
}

.share {
  width: 30rpx;
  height: 30rpx;
}